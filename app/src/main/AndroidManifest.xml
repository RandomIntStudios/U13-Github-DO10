<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.ur.mi.android.chooseyourownadventure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="de.ur.mi.android.chooseyourownadventure.scenes.Start"
            android:label="@string/label_start"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.ur.mi.android.chooseyourownadventure.scenes.Stream"
            android:label="@string/label_badEnding"
            android:exported="false">
        </activity>
        <activity
            android:name="de.ur.mi.android.chooseyourownadventure.scenes.GoodEnding"
            android:label="@string/label_godEnding"
            android:exported="false">
        </activity>
        <activity
            android:name="de.ur.mi.android.chooseyourownadventure.scenes.BadEnding"
            android:label="@string/label_badEnding"
            android:exported="false">
        </activity>

    </application>

</manifest>